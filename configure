  if test -x $CCA_PATH_TO_CONFIG ; then
    p2cv_config=$CCA_PATH_TO_CONFIG
    p2cv_path=`$CCA_PATH_TO_CONFIG --var CCASPEC_INSTALL_DIR`
    p2cv_result=$p2cv_path
    CCAFE_CSB_TEST=$p2cv_result
    CCAFE_CSB_CONFIG_TEST=$p2cv_config
    p2cv_ok=yes
    if test -n 'CCAFE_CSB="$CCAFE_CSB_TEST"
                CCAFE_CSB_CONFIG="$CCAFE_CSB_CONFIG_TEST" ' ; then
      # if found
      CCAFE_CSB="$CCAFE_CSB_TEST"
                CCAFE_CSB_CONFIG="$CCAFE_CSB_CONFIG_TEST"
    fi
  else
    { echo "$as_me:$LINENO: WARNING: Ignoring env CCA_PATH_TO_CONFIG pointing to non-executable $CCA_PATH_TO_CONFIG." >&5
echo "$as_me: WARNING: Ignoring env CCA_PATH_TO_CONFIG pointing to non-executable $CCA_PATH_TO_CONFIG." >&2;}
  fi
