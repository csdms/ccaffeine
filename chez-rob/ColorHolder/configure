    no)
        # lUser input error
        with_cca=_searching
        ;;
    yes)
        #  User didn't give the option or didn't give useful
        #  information, search for it ourselves
        with_cca=_searching
        ;;
    *)
        CCA_ROOT="$with_cca"
        ;;
esac

if test "$with_cca" = _searching ; then
    echo $ac_n "checking for REQUIRED cca-spec""... $ac_c" 1>&6
echo "configure:978: checking for REQUIRED cca-spec" >&5
    for chome in $hwmfc_cca_spec_dirs ; do
        if test -d $chome ; then
            with_cca=$chome
            echo "$ac_t""found $chome." 1>&6
            break
        fi
    done
fi
if test "$with_cca" = _searching ; then
    echo "$ac_t""Cannot find CCA specification root directory." 1>&6
    { echo "configure: error: Please install it and/or use --with-cca=CCA_ROOT." 1>&2; exit 1; }
fi
